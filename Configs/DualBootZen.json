{
  "metadata": {
    "name": "Dual Boot Zen Kernel Configuration",
    "description": "Dual-boot setup with existing Windows using Zen kernel and ZFS",
    "author": "sqazi",
    "version": "1.0.0"
  },
  "system": {
    "default_username": "",
    "default_hostname": "arch-z13",
    "default_timezone": "",
    "locale": "en_US.UTF-8"
  },
  "installation": {
    "dual_boot_mode": "gpt",
    "kernel_variant": "zen",
    "default_filesystem": "zfs",
    "default_desktop": "omarchy",
    "enable_gaming": true,
    "enable_snapshots": true,
    "enable_secure_boot": false,
    "enable_suspend_resume": true,
    "enable_hibernate": true
  },
  "power": {
    "enable_power_management": true,
    "enable_tlp": true,
    "enable_asusctl": true,
    "tdp_profiles": {
      "efficient": 7,
      "ai": 45,
      "gaming": 93,
      "maximum": 120
    },
    "battery_care": {
      "start_charge_threshold": 40,
      "stop_charge_threshold": 80
    }
  },
  "hardware": {
    "enable_hardware_fixes": true,
    "enable_wifi_stability_fixes": true,
    "enable_audio_firmware": true,
    "enable_touchpad_fixes": true,
    "enable_180hz_display": true,
    "enable_variable_refresh_rate": true
  },
  "recovery": {
    "enable_error_recovery": true,
    "enable_filesystem_fallback": true,
    "enable_fresh_reinstall": true,
    "enable_detached_mode": true
  }
}
